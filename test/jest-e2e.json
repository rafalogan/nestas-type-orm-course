{
	"moduleFileExtensions": [
		"js",
		"json",
		"ts"
	],
	"rootDir": ".",
	"roots": [
		"<rootDir>/../src",
		"<rootDir>"
	],
	"modulePaths": [
		"<rootDir>/src",
		"<rootDir>"
	],
	"moduleNameMapper": {
		"src(.*)$": "<rootDir>/../src$1",
		"test/mocks(.*)$": "<rootDir>/mocks$1"
	},
	"testEnvironment": "node",
	"testRegex": ".e2e-spec.ts$",
	"transform": {
		"^.+\\.(t|j)s$": "ts-jest"
	}
}
